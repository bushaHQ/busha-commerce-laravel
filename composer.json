{
    "name": "busha/commerce",
    "description": "Official Laravel Wrapper for the Busha Commerce API",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Busha\\Commerce\\": "src/",
            "Busha\\Commerce\\Tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Busha",
            "email": "hello@busha.co"
        }
    ],
    "keywords": [
        "github",
        "laravel",
        "php",
        "busha",
        "busha commerce",
        "fintech",
        "crypto",
        "payments",
        "open source",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "laravel 9"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Busha\\Commerce\\BushaServiceProvider"
            ],
            "aliases": {
                "BushaCommerce": "Busha\\Commerce\\Facades\\BushaCommerce"
            }
        }
    },
    "scripts" : {
        "test" : "./vendor/bin/phpunit"
    },
    "require": {
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "guzzlehttp/guzzle": "~6|~7",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "dev-develop",
        "phpunit/phpunit": "10.1.x-dev"
    }
}
